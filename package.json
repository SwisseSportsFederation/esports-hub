{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -i ./styles/tailwind.scss -o ./app/tailwind.css --minify",
    "dev": "concurrently \"npm run dev:css\" \"node -r dotenv/config node_modules/.bin/remix dev\"",
    "dev:css": "postcss styles/tailwind.scss --output app/styles/tailwind.css -w",
    "postinstall": "remix setup node",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "@prisma/client": "^3.8.1",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@remix-run/server-runtime": "^1.1.3",
    "@types/lru-cache": "^5.1.1",
    "classnames": "^2.3.1",
    "dotenv": "^14.2.0",
    "lru-cache": "^6.0.0",
    "postcss-scss": "^4.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-transition-group": "^4.4.2",
    "remix": "^1.1.3",
    "remix-auth": "^3.2.1",
    "remix-auth-auth0": "^1.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-transition-group": "^4.4.4",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "prisma": "^3.8.1",
    "tailwindcss": "^3.0.15",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
