{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/tailwind.scss -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/tailwind.scss -o app/styles/app.css",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.7",
    "@prisma/client": "^4.8.0",
    "@remix-run/node": "^1.9.0",
    "@remix-run/react": "^1.9.0",
    "@remix-run/server-runtime": "^1.9.0",
    "@remix-run/vercel": "^1.9.0",
    "@vercel/node": "^2.6.2",
    "classnames": "^2.3.2",
    "lru-cache": "^7.14.1",
    "prisma": "^4.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-transition-group": "^4.4.5",
    "remix-auth": "^3.4.0",
    "remix-auth-auth0": "^1.5.1",
    "zod": "^3.20.2",
    "zodix": "^0.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@remix-run/dev": "^1.9.0",
    "@remix-run/eslint-config": "^1.9.0",
    "@remix-run/serve": "^1.9.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-transition-group": "^4.4.5",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.27.0",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  }
}
